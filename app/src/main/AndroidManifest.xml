<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.menubee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Menubee">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".TextOrder"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name=".Tip" android:label="tip"/>
        <activity android:name=".Choice_mode" android:label="choice_mode"/>
        <activity android:name=".AdditionalOrder" android:label="additionalorder"/>
        <activity android:name=".Cafe" android:label="cafe"/>
    </application>

</manifest>